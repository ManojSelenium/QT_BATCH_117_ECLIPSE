<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="classes">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Test">
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<classes>
			<class name="com.testcases.FirstTest" />
			<class name="com.testcases.SecondTest" />
			<class name="com.testcases.ThirdTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
